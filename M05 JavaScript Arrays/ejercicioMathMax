  
  var array = [4, 2, 3];

  console.log (array);

  var array1 = [1, 3, 4];

  console.log (array1);

  var array2 = array.concat (array1); 

  console.log (array2);

  var dupArray = [];

  var provArray = array2.sort (); 

  console.log (array2);    

  for (let i = 0; i < array2.length; i++) {

    if (provArray [i + 1] === provArray [i]) {

      dupArray.push (provArray [i]);

    }

  }
  
  console.log (dupArray);

  